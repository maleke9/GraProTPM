package components;
import components.lib.average;
import components.lib.Integrator;
import components.lib.percentCompare;

static class Component
reads veloCar.vfl, distanceCar.sfl, distanceCar.sfr, distanceCar.srl, distanceCar.srr, veloCar.vfr, veloCar.vrl, veloCar.vrr
writes distanceCar.avgDistanceCar, distanceCar.sfl, distanceCar.sfr, distanceCar.srl, distanceCar.srr {
	average average_instance;
	@dT
	public real deltatime = 0.0;
	Integrator Integrator_instance;
	Integrator Integrator_instance1;
	Integrator Integrator_instance2;
	Integrator Integrator_instance3;
	percentCompare percentCompare_instance;
	percentCompare percentCompare_instance1;
	percentCompare percentCompare_instance2;
	percentCompare percentCompare_instance3;
	
	

	@generated("blockdiagram")
	public void calc() {
		Integrator_instance.integrate(veloCar.vfl, deltatime); // Main/calc 1
		distanceCar.avgDistanceCar = average_instance.calc(distanceCar.sfl, distanceCar.sfr, distanceCar.srl, distanceCar.srr); // Main/calc 2
		distanceCar.sfl = Integrator_instance.outVal(); // Main/calc 3
		distanceCar.sfr = Integrator_instance1.outVal(); // Main/calc 4
		distanceCar.srl = Integrator_instance2.outVal(); // Main/calc 5
		distanceCar.srr = Integrator_instance3.outVal(); // Main/calc 6
		Integrator_instance1.integrate(veloCar.vfr, deltatime); // Main/calc 7
		Integrator_instance2.integrate(veloCar.vrl, deltatime); // Main/calc 8
		Integrator_instance3.integrate(veloCar.vrr, deltatime); // Main/calc 9
	}
}