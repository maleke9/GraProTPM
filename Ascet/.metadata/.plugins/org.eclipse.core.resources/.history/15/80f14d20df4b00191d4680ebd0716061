package components.lib;

type myInt is integer 1 .. 10000;
class TirePressureProject {
	real v;
	integer mem;
	real v_2;

	@generated("blockdiagram")
	@error("blockdiagram")
	public real calc(myInt in arg, myInt in arg_2, myInt in arg_3, myInt in scaleGain) {
		mem = (((mem * arg) + arg_2) % arg_3); // Main/calc 1
	}
}