import components.lib.average;


static class AverageTest {
	average avg;
		
	/*
	 * check Fusi:
	 * 1.	Turn the ACC on
		2.	Trigger Crash
		3.	Check if ACC is turned off
	 
	 */
	@Test
	public void testAverage() {
	
		Assert.assertTrue(avg.calc(1.0,2.0,3.0,4.0)==2.5)

	}
}