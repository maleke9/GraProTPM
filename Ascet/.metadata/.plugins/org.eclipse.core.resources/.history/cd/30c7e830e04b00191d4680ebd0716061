package components.lib;

type myInt is integer 1 .. 10000;
class TirePressureProject {
	myInt mem = 1;

	@generated("blockdiagram")
	public real calc(myInt in argA, myInt in argC, myInt in arg_3, real in scaleGain) {
		mem = (((mem * argA) + argC) % arg_3); // Main/calc 1
		return(real(mem) * scaleGain); // Main/calc 2
	}
}