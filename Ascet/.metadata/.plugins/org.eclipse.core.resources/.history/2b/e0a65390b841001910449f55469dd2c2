package components.lib;

class Integrator {
	real memory;
	characteristic real kI = 4.0;

	@generated("blockdiagram")
	 void integrate(real in dtVal, real in inputVal) {
// Main/integrate 1
		memory = ((inputVal * kI * dtVal) + memory);
	}

	@generated("blockdiagram")
	 real outVal() {
// Main/outVal oo
		return memory;
	}
}